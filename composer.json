{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.6.3",
        "intervention/image": "^2.5",
        "kiritokatklian/nova-permission": "^4.0",
        "laravel/framework": "^9.35",
        "laravel/jetstream": "^2.3",
        "laravel/nova": "~4.16.1",
        "laravel/nova-log-viewer": "^0.1.2",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "opcodesio/log-viewer": "^1.6",
        "spatie/laravel-permission": "^5.5",
        "tightenco/ziggy": "^1.4",
        "company-api/custom-js-css": "*",
        "company-api/date-filter": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaTheme"
        },
        {
            "type": "path",
            "url": "./nova-components/CustomDateFilter"
        },
        {
            "type": "path",
            "url": "./nova-components/CustomJsCss"
        },
        {
            "type": "path",
            "url": "./nova-components/DateFilter"
        }
    ]
}